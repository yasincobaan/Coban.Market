
@{
    ViewBag.Title = "Index";
}



@Styles.Render("~/daterangepicker/css")
@Styles.Render("~/category/index/css")



@Html.Partial("_PartialSearchBox")
@Html.Partial("_PartialDatatable")




@Scripts.Render("~/category/index/js")
@Scripts.Render("~/daterangepicker/js")
@Scripts.Render("~/dateRangeStart/js")

<div class="modal fade" id="editModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">
                    Bu sayfa veri kaybına yol açabilir.
                </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form group row">
                    <div class="control-label col-md-4">Title</div>
                    <div class="col-md-8">
                        <input type="text" name="Title" class="form-control Title" id="Title" />
                    </div>
                </div>
                <br />
                <div class="form group row">
                    <div class="control-label col-md-4">Description</div>
                    <div class="col-md-8">
                        <input type="text" name="Description" class="form-control Title" id="Description" />
                    </div>
                </div>
                <br />
                <div class="input-group mb-3">
                    <div class="input-group-prepend">
                        <span class="input-group-text"><i class="fa fa-upload"></i></span>
                    </div>
                    <div class="custom-file">
                        <input type="file" class="custom-file-input Image" id="Image2" name="Image2">
                        <label class="custom-file-label" for="Image2">Dosya Seçiniz</label>
                    </div>
                </div>
                <img src="" name="Image" class=" col-md-12 img-responsive Image" id="Image" style="padding: 30px;" />
             
               
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </div>
</div>